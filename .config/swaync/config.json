{
  "border_radius": 10,
  "padding": 8,
  "background_color": "#191724",
  "text_color": "#E0DEF4",
  "highlight_color": "#EB6F92",
  "timeout": 5000,
  "max_notifications": 5,
  "ignore_dnd": false,
  "font": "JetBrainsMono NF ExtraBold 13",
  "icons": {
    "urgent": "",
    "normal": "",
    "low": ""
  },
  "position": "top_right",
  "actions": {
    "clear_on_click": true,
    "close_after_view": true
  },
  "animations": {
    "enabled": true,
    "type": "fade",
    "duration": 300
  },
  "notification_opacity": 0.9,
  "shadow": true,
  "shadow_color": "#000000AA"
}
